
public class testeSistema {

	public static void main(String[] args) {
		
		gerente g = new gerente();
		g.setSenha(2222);
	
		admin adm = new admin();
		adm.setSenha(3333);
		
		designer d = new designer();
		d.setSenha(4444);
		
		sistemaInterno si = new sistemaInterno();
		si.autentica(g);
		si.autentica(adm);
		si.autentica(d);
	}
	
}
