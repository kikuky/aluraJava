
public class Cliente implements Autenticavel {

	private Assinatura Ass;
	
	public Cliente() {
		
		this.Ass = new Assinatura();
		
	}
	
	@Override
	public void setSenha(int Senha) {
		this.Senha = Senha;
		
	}

	@Override
	public boolean Autentica(int Senha) {
		
		if (this.Senha == Senha) {
			
			return true;
			
		} else {
			
			return false;
			
		}
		
	}
}
