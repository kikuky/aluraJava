public class Admin extends Funcionario implements Autenticavel {
	
	private int Senha;
	
	@Override
	public double getBonificacao() {
		return 50;
	}

	@Override
	public void setSenha(int Senha) {
		
		this.Senha = Senha;
		
	}

	@Override
	public boolean Autentica(int Senha)  {
		
		if (this.Senha == Senha) {
			
			return true;
			
		} else {
			
			return false;
			
		}
		
	}
		
}
